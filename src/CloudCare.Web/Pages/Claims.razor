@page "/Claims"
<h3>Claims</h3>
<AuthorizeView>
    <NotAuthorized>
        <h1>SYBAU!</h1>
    </NotAuthorized>
    <Authorized>
        <h2>
            <ul>
                @foreach (var claim in context.User.Claims)
                {
                    <li><b>@claim.Type</b>: @claim.Value</li>
                }
            </ul>

        </h2>
    </Authorized>
</AuthorizeView>

